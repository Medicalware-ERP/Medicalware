{% apply spaceless %}
    <div class="d-flex justify-content-center">
        <a class="btn btn-xs btn-primary margin-right-1" href="{{ path('show_room_information', {id: room.id}) }}"><i class="fa fa-eye"></i></a>
        <a class="btn btn-xs btn-primary btn-edit-room margin-right-1" data-room-id="{{ room.id }}"><i class="fa fa-pen"></i></a>
        <a class="btn btn-xs btn-danger btn-delete-room" data-room-id="{{ room.id }}"><i class="fa fa-trash-alt"></i></a>
    </div>
{% endapply %}